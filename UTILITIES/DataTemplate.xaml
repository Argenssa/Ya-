<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Ya_.VIEWMODEL"
                    xmlns:view="clr-namespace:Ya_.VIEW">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SongsVM}">
        <view:Songs />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:StatisticsVM}">
        <view:Statistics/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SongManagerVM}">
        <view:SongManager/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProfileVM}">
        <view:Profile/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SearchVM}">
        <view:Search/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ComplitationVM}">
        <view:ComplitationManager/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DelWindowVM}">
        <view:DelWindow/>
    </DataTemplate>

</ResourceDictionary>